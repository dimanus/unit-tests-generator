{
    "name": "kassko/test-unit-tests-gen",
    "description": "A unit tests generator",
    "license": "MIT",
    "keywords": ["test", "phpunit", "stub", "spy", "mock"],
    "require": {
        "php": ">=5.5",
        "nikic/php-parser": "^3.0.2",
        "kassko/util-member-accessor": "dev-master",
        "symfony/filesystem": "^3.1",
        "symfony/finder": "^3.1"
    },
    "require-dev": {
        "doctrine/annotations": "^1.2",
        "phpunit/phpunit": "4.6.*",
        "phpunit/phpunit-mock-objects": "2.3.0"
    },
    "suggest": {
        "doctrine/annotations": "If using annotations to describe unit tests"
    },
    "autoload": {
        "psr-4": {
            "Kassko\\Test\\UnitTestsGenerator\\": "lib"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
